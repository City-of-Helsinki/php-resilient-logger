{
    "name": "city-of-helsinki/php-resilient-logger",
    "description": "PHP resilient logger for monolog",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "phpspec/prophecy-phpunit": "^2.3",
        "monolog/monolog": "^3.9",
        "elasticsearch/elasticsearch": "^8"
    },
    "require-dev": {
        "behat/mink": "^1.12",
        "phpspec/prophecy": "^1.20",
        "phpspec/prophecy-phpunit": "^2.3",
        "phpunit/phpunit": "^10.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
		"psr-4": {
			"ResilientLogger\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "ResilientLogger\\Tests\\": "tests/src/"
        }
    }
}
